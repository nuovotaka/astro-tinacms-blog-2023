    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "reading-time": "^1.5.0",
    "rehype-katex": "^6.0.3",
    "remark-emoji": "^3.1.2",
    "remark-math": "^5.1.1",
